`timescale 1ns/1ps
//Contador de Programa

module program_counter_tb();

reg clk, pc_src;
reg signed [31:0] sign_imm;
wire [31:0] pc;

initial
begin
	clk = 1'b0;
	pc_src = 1'b0;
	sign_imm = -6
	
	#200
	pc_src = 1'b1;
	

end


always #10 clk = ~clk;

module program_counter #(parameter NUM_BITS_ADDR = 32)
(
	.clk(clk), 
	.pc_src(pc_src),
	.sign_imm(sign_imm) ,
	.pc(pc)

);